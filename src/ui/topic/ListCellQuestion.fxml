<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="cellLayout" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <right>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="19.0" fitWidth="18.0" onMouseClicked="#like" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../img/thumbs-up.png" />
               </image>
            </ImageView>
            <Label fx:id="nbLike" text="Label">
               <padding>
                  <Insets right="20.0" />
               </padding>
            </Label>
            <Button fx:id="seeMore" mnemonicParsing="false" onMouseClicked="#seeMore" text="See more" />
         </children>
      </HBox>
   </right>
   <left>
      <Label fx:id="questionContent" text="Label" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
