<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="175.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Pane prefHeight="171.0" prefWidth="350.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="nameTopic" layoutX="142.0" layoutY="76.0" />
            <Label layoutX="14.0" layoutY="76.0" prefHeight="25.0" prefWidth="107.0" style="-fx-text-fill: #357AB7;" text="Topic Name">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="120.0" layoutY="14.0" style="-fx-text-fill: #357AB7;" text="Update Topic">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button layoutX="142.0" layoutY="122.0" mnemonicParsing="false" onAction="#updateTopic" prefHeight="25.0" prefWidth="66.0" style="-fx-background-color: #357AB7; -fx-text-fill: white;" text="Save" />
         </children>
      </Pane>
   </left>
</BorderPane>
